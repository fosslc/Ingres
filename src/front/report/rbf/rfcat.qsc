				coldesc->deleted = FALSE;

				coldesc++;
			}
##		}

		if (IIRFcdcount != 0)
		{
			dcoldesc = del_cols;

			for (i = 0; i < IIRFcdcount; i++)
			{
				STcopy(dcoldesc->name, coldesc->name);
				coldesc->datatype = dcoldesc->datatype;
				coldesc->length = dcoldesc->length;
				coldesc->prec = dcoldesc->prec;
				coldesc->deleted = TRUE;

				coldesc++;
