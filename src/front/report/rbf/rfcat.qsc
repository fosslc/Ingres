		**  left.  If everything has been deleted, issue error
		**  and resume.
		*/
		if (IIRFcccount == IIRFcdcount)
		{
			IIUGerr(E_RF00A8_NoColsLeft, UG_ERR_ERROR, 0);
##			resume
		}

		/*
		**  Unload the values.
		*/
		coldesc = IIRFcollist;
##		unloadtable rfchoose_columns colnames (cname = colnames,
##			db_len = len, db_dt = dt, db_prec = prec,
##			rowstate = _STATE)
