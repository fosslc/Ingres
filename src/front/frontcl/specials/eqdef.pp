# define II_LQTYPE_PTR          unusual_ptr
# define II_LQTYPE_VOID         unusual_void
# define II_LQTYPE_GPTR		unusual_generic_pointer
#endif /* SOME_UNUSUAL_SYSTEM */


# ifdef WIN16
# define II_LQTYPE_BOOL         int
# define II_LQTYPE_I4           long
