with text_io;
package io_interface is
 package int_io is new text_io.integer_io(integer);
 package short_int_io is new text_io.integer_io(short_integer);
 package long_int_io is new text_io.integer_io(long_integer);
 package float_io is new text_io.float_io(float);
 package long_float_io is new text_io.float_io(long_float);
end io_interface;
package body io_interface is
end;
