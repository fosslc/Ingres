		  CCMD=tcio CBLOCK=20 \
		  CUNLOAD=true \
		  CSKIP= CREWIND= CRETENSION= \
		  TAPE1=/dev/tape0/1600nr TAPE1N=/dev/tape0/1600nr \
		  TAPE1L=/dev/tape0/1600r \
