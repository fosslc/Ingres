	LIBQ_LIBS 	= $(UILIB) $(COPYLIB) $(GCFLIB) $(CUFLIB) ;
	
	COMPAT_LIBS	= $(IMPCOMPATLIB) $(IMPCOMPATLIBDATA) ;
	
