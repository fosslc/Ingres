ESQLPATTERN = "^[ 	]*EXEC SQL INCLUDE[ 	]*'(.*)'" 
	      "^[ 	]*EXEC SQL INCLUDE[ 	]*<(.*)>" 
	      "^[ 	]*exec sql include[ 	]*<(.*)>" 
	      "^[ 	]*EXEC SQL INCLUDE[ 	]*([A-Za-z]*)"
	      "^[ 	]*exec sql include[ 	]*([A-Za-z]*)" ;
