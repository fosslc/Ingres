	 $(QEFLIBHB) $(ASCFLIBHB) $(AWSFLIBHB) $(ADFLIBHB) $(DDFLIBHB) $(RQFLIBHB)
	 $(SXFLIBHB) $(TPFLIBHB) $(APILIBHB) ;
}

if $(NT)
