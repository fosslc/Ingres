	}

	STcopy(du_dbenv->du_jnlloc.du_locname, jnlloc);
	STcopy(du_dbenv->du_jnlloc.du_area, areaname);
	status_value = DU_JNLS_LOC;
##	append iilocations(lname=jnlloc, area=areaname, status = status_value);

