DU_STATUS
duc_1makedbdb(du_dbenv, duc_errcb)
DU_ENV		*du_dbenv;
DU_ERROR	*duc_errcb;
##  {
##	int	row_cnt;
##	char	database_name[DB_MAXNAME+4];
##	char	owner[DB_MAXNAME+4];
##	char	dbloc[DB_MAXNAME+4];
##	char	jnlloc[DB_MAXNAME+4];
